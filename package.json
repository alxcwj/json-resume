{
  "name": "jsonresume-theme-academic-professional",
  "version": "1.0.0",
  "description": "An academic-oriented JSON Resume theme based on jsonresume-theme-professional v1.1.0",
  "main": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node build.js",
    "serve": "npm run build && resume serve --theme .",
