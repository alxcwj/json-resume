  },
  "homepage": "https://github.com/alxcwj/json-resume#readme",
  "dependencies": {
    "prismjs": "^1.29.0",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.0.1",
    "styled-components": "^6"
  },
  "devDependencies": {
    "esbuild": "^0.27.3",
    "playwright": "^1.58.2",
    "resume-cli": "^3.0.8"
  }
